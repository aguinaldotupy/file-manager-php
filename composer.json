{
    "name": "tupy/filemanager",
    "description": "Laravel File Manager Morph table",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aguinaldo Tupy",
            "email": "aguinaldo.tupy@gmail.com"
        }
    ],
    "require": {
        "php" : "^7.2",
        "ext-zip": "*",
        "illuminate/config": "^6.0|^7.0",
        "illuminate/http": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0",
        "illuminate/console": "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0",
        "intervention/image": "^2.4",
        "illuminate/filesystem": "^6.0|^7.0",
        "illuminate/routing": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Tupy\\FileManager\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.2-dev"
        },
        "laravel": {
            "providers": [
                "Tupy\\FileManager\\FileManagerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
