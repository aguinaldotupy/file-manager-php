{
    "name": "tupy/filemanager",
    "description": "Laravel File Manager Morph table",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aguinaldo Tupy",
            "email": "aguinaldo.tupy@gmail.com"
        }
    ],
    "require": {
        "php" : "^7.2",
        "ext-zip": "*",
        "illuminate/config": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "illuminate/http": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "illuminate/console": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "intervention/image": "^2.4",
        "illuminate/filesystem": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "illuminate/routing": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Tupy\\FileManager\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Tupy\\FileManager\\FileManagerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}